<?xml version="1.0" ?>
<sdf version="1.6">
  <world name="manipulation_world">
    <!-- Include ground plane -->
    <include>
      <uri>model://ground_plane</uri>
    </include>

    <!-- Include sun -->
    <include>
      <uri>model://sun</uri>
    </include>

    <!-- Boundary walls -->
