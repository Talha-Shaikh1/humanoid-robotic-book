              <size>8 0.2 1</size>
            </box>
          </geometry>
        </collision>
        <visual name="visual">
          <geometry>
            <box>
              <size>8 0.2 1</size>
            </box>
          </geometry>
          <material>
            <ambient>0.5 0.5 0.5 1</ambient>
            <diffuse>0.7 0.7 0.7 1</diffuse>
            <specular>0.1 0.1 0.1 1</specular>
          </material>
        </visual>
        <inertial>
          <mass>10.0</mass>
          <inertia>
            <ixx>10.683333</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>53.333333</iyy>
            <iyz>0</iyz>
            <izz>64.0</izz>
          </inertia>
        </inertial>
      </link>
    </model>

